# Sample environment variables for testing migration
NODE_ENV=development
PORT=3000
PROTOCOL=http
HOST=localhost

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=postgres
DATABASE_USER=postgres
DATABASE_PASSWORD=dev-password-change-me
DATABASE_URL=postgresql://postgres:dev-password-change-me@localhost:5432/postgres

# Cache
REDIS_URL=redis://localhost:6379

# EventStore
ESDB_CONNECTION_STRING=esdb://localhost:2113?tls=false

# Authentication
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=gs-scaffold
KEYCLOAK_CLIENT_ID=gs-scaffold-api
KEYCLOAK_CLIENT_SECRET=dev-client-secret-change-me
JWT_AUDIENCE=gs-scaffold-api

# Security
PII_ENCRYPTION_KEY=dev-encryption-key-32-chars-change-in-prod
OPA_URL=http://localhost:8181

# Logging
LOG_LEVEL=debug
LOG_SINK=console
PRETTY_LOGS=true

# App
APP_NAME=gs-scaffold
APP_VERSION=0.0.1
