{
  "id": null,
  "title": "NestJS Logs Overview",
  "tags": ["logs", "loki", "nestjs"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "logs",
      "title": "Log Table",
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [{ "expr": "{app=\"gs-scaffold\"}", "refId": "A" }],
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 10 }
    },
    {
      "type": "timeseries",
      "title": "Errors Over Time",
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "count_over_time({app=\"gs-scaffold\"} |= \"error\" [1m])",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 0, "y": 10, "w": 12, "h": 8 }
    },
    {
      "type": "piechart",
      "title": "Log Level Distribution",
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (level) (count_over_time({app=\"gs-scaffold\"}[5m]))",
          "refId": "C"
        }
      ],
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 8 }
    }
  ]
}
