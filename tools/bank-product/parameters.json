  "tables": {
    "rail": {
      "docs": {
        "name": "Rails",
        "summary": "Defines the `core banking rails` used for transactions, including their attributes and configurations."
      },
      "apis": {
        "reset/:stream": {
          "method": "POST",
          "params": {
            "stream": {
              "type": "string",
              "required": true,
              "description": "The ID of the projection to retrieve and reset. This parameter specifies which projection stream will be targeted by the reset operation."
            }
          },
          "responses": {
            "200": {
              "description": "The projection was reset successfully. The operation completed without errors and the specified projection stream is now in its initial state."
            }
          },
          "description": "Resets a specific projection stream to its initial state. Use this endpoint to reinitialize a single projection by its stream identifier."
        },
        "reset-all": {
          "method": "POST",
          "responses": {
            "200": {
              "description": "All projections were reset successfully. The operation completed without errors and all projection streams are now in their initial state."
            }
          },
          "description": "Resets all projection streams in the system to their initial state. Use this endpoint for bulk reinitialization of all projections."
        },
        "metrics": {
          "method": "GET",
          "responses": {
            "200": {
              "description": "Metrics for all projections were retrieved successfully."
            }
          },
          "description": "Retrieves metrics for all projection streams in the system."
        }
      },
      "store": {
        "read": "eventstream",
        "write": "eventstream",
        "list": "redis"
      },
      "cancel": {
        "create": false,
        "update": false,
        "delete": false,
        "batch": false,
        "get": false
      },
      "complete": [],
